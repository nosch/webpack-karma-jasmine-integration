{
  "name": "webpack-karma-jasmine-integration",
  "version": "0.0.1",
  "description": "Webpack-Karma-Jasmine integration, very simple dev stack",
  "main": "./src/index.js",
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/karma start",
    "karma": "./node_modules/.bin/karma start --auto-watch --no-single-run --browsers PhantomJS",
    "dist": "NODE_ENV=production ./node_modules/.bin/webpack --progress --config ./webpack.production.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nosch/webpack-karma-jasmine-integration.git"
  },
  "homepage": "https://github.com/nosch/webpack-karma-jasmine-integration",
  "author": {
    "name": "nosch",
    "email": "norbert.schmidt@mayflower.de",
    "url": "https://github.com/nosch"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.24",
    "css-loader": "0.15.6",
    "extract-text-webpack-plugin": "0.8.2",
    "html-webpack-plugin": "1.6.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "0.5.3",
    "phantomjs": "^1.9.18",
    "postcss-loader": "0.6.0",
    "raw-loader": "0.5.1",
    "style-loader": "0.12.4",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.11.0"
  },
  "dependencies": {
    "jquery": "latest"
  }
}
